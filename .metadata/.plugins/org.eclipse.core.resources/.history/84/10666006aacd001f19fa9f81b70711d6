package com.stsdemo;

public class Swimmer extends Olympian{
	//specific attribute for swimmer class
	private int swimSpeed;


	//constructor for child class 
	public Swimmer(int swimSpeed,String name) {
		super(name);
		this.swimSpeed = swimSpeed;
	}
	
	@Override
	public void displayInfo() {
		System.out.println(String.format("Name : %s \n Energy : %s \n Speed : %s", this.getName(),this.getEnergy(),this.swimSpeed));
	}

}
